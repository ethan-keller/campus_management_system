<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="564.0" prefWidth="911.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.demo.controllers.AdminManageUserViewController">
   <bottom>
      <HBox alignment="CENTER" prefHeight="78.0" prefWidth="911.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#createNewUserClicked" prefHeight="35.0" prefWidth="93.0" text="New" textFill="#0ebaf8">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#editUserClicked" prefHeight="35.0" prefWidth="95.0" text="Edit" textFill="#0ebaf8">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteUserClicked" prefHeight="35.0" prefWidth="87.0" text="Delete" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="587.0" layoutY="32.0" mnemonicParsing="false" onAction="#historyClicked" prefHeight="35.0" prefWidth="93.0" text="History" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="520.0" layoutY="32.0" mnemonicParsing="false" onAction="#bikeClicked" prefHeight="35.0" prefWidth="93.0" text="Bikes" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backClicked" prefHeight="35.0" prefWidth="93.0" text="Back" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <TableView fx:id="userTable" prefHeight="500.0" prefWidth="900.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="usernameColumn" maxWidth="350.0" minWidth="100.0" prefWidth="218.0" text="Username" />
          <TableColumn fx:id="userTypeColumn" minWidth="200.0" prefWidth="280.0" text="Type" />
            <TableColumn fx:id="userPasswordColumn" minWidth="90.0" prefWidth="411.0" text="Password" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <top>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="54.0" prefWidth="150.0" text="Users" textAlignment="CENTER" textFill="#0ebaf8" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </top>
</BorderPane>
