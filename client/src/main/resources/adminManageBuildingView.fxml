<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="564.0" prefWidth="911.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.demo.controllers.AdminManageBuildingViewController">
   <bottom>
      <HBox alignment="CENTER" prefHeight="78.0" prefWidth="911.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#createNewBuildingClicked" prefHeight="35.0" prefWidth="93.0" text="New" textFill="#0ebaf8">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#editBuildingClicked" prefHeight="35.0" prefWidth="93.0" text="Edit" textFill="#0ebaf8">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteBuildingClicked" prefHeight="35.0" prefWidth="93.0" text="Delete" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backClicked" prefHeight="35.0" prefWidth="93.0" text="Back" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="signOutButton" mnemonicParsing="false" onAction="#signOutButtonClicked" prefHeight="35.0" prefWidth="96.0" text="Sign Out" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <TableView fx:id="buildingTable" prefHeight="500.0" prefWidth="900.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="buildingIdColumn" minWidth="100.0" prefWidth="-1.0" text="ID" />
          <TableColumn fx:id="buildingNameColumn" minWidth="200.0" prefWidth="279.1999816894531" text="Name" />
            <TableColumn fx:id="buildingRoomCountColumn" minWidth="90.0" prefWidth="170.4000244140625" text="Room count" />
            <TableColumn fx:id="buildingAddressColumn" minWidth="200.0" prefWidth="329.59991455078125" text="Address" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <top>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="54.0" prefWidth="150.0" text="Buildings" textAlignment="CENTER" textFill="#0ebaf8" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </top>
</BorderPane>
