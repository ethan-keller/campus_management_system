<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="564.0" prefWidth="911.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.demo.controllers.AdminUserHistoryViewController">
   <bottom>
      <HBox alignment="CENTER" prefHeight="78.0" prefWidth="911.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#createNewBookingClicked" prefHeight="35.0" prefWidth="93.0" text="New" textFill="#0ebaf8">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteBookingClicked" prefHeight="35.0" prefWidth="87.0" text="Delete" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="412.0" layoutY="32.0" mnemonicParsing="false" onAction="#foodClicked" prefHeight="35.0" prefWidth="87.0" text="Food" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backClicked" prefHeight="35.0" prefWidth="93.0" text="Back" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="signOut" mnemonicParsing="false" onAction="#signOutClicked" text="Sign Out" textFill="#0ebaf8">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <TableView fx:id="bookingTable" prefHeight="500.0" prefWidth="900.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="bookingIdColumn" maxWidth="350.0" minWidth="100.0" prefWidth="140.27841246128082" text="Booking Id" />
            <TableColumn fx:id="bookingDateColumn" maxWidth="350.0" minWidth="100.0" prefWidth="140.27841246128082" text="Date" />
            <TableColumn fx:id="bookingRoomColumn" minWidth="90.0" prefWidth="215.72161102294922" text="Room" />
            <TableColumn fx:id="bookingStartColumn" minWidth="149.48129272460938" prefWidth="263.0380554199219" text="Starting Time" />
            <TableColumn fx:id="bookingEndColumn" minWidth="90.0" prefWidth="289.9619140625" text="Ending Time" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <top>
      <HBox alignment="CENTER">
         <children>
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="54.0" prefWidth="440.0" text="Booking History of user: " textAlignment="CENTER" textFill="#0ebaf8">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Label fx:id="usernameLabel" contentDisplay="CENTER" prefHeight="54.0" prefWidth="200.0" textFill="#0ebaf8">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <padding>
                  <Insets left="20.0" />
               </padding>
            </Label>
         </children>
      </HBox>
   </top>
</BorderPane>
