                   <Text fx:id="bikeStartingTime" strokeType="OUTSIDE" strokeWidth="0.0" text="Start: " />
                   <Text fx:id="bikeEndingTime" strokeType="OUTSIDE" strokeWidth="0.0" text="End: " />
